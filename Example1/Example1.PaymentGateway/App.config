<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="UnicastBusConfig" type="NServiceBus.Config.UnicastBusConfig, NServiceBus.Core"/>
  </configSections>

  <UnicastBusConfig>
    <MessageEndpointMappings>
      <add Messages="Example1.Contracts.Sales.OrderPlacedEvent, Example1.Contracts" Endpoint="Example1.Sales" />
    </MessageEndpointMappings>
  </UnicastBusConfig>

  <appSettings>
	  <add key="EndpointConfigurationType" value="Example1.PaymentGateway.EndpointConfig, Example1.PaymentGateway"/>
  </appSettings>

<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/></startup></configuration>
